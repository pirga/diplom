@{
    Layout = null;
    var newFileName = "";
    var imagePath = "";
    if (IsPost)
    {
        var temp = Request.Files;
        for (int i = 0; i < Request.Files.Count; i++)
        {
            newFileName = Guid.NewGuid().ToString() + "_" + Path.GetFileName(Request.Files.Get(i).FileName);
            var pathToApp = AppDomain.CurrentDomain.BaseDirectory;
            imagePath = pathToApp + @"Images\" + newFileName;
            Request.Files[i].SaveAs(imagePath);
        }
    }
}
<!DOCTYPE html>
<html>
<head>
    <title>Image Upload</title>
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/css/bootstrap.min.css" rel="stylesheet">
    <link href="../../Content/fileinput.css" media="all" rel="stylesheet" type="text/css" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" media="all" rel="stylesheet" type="text/css" />
    <link href="../../Content/themes/explorer-fa/theme.css" media="all" rel="stylesheet" type="text/css" />
    <script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
    <script src="../../Scripts/plugins/sortable.js" type="text/javascript"></script>
    <script src="../../Scripts/fileinput.js" type="text/javascript"></script>
    <script src="../../Scripts/locales/fr.js" type="text/javascript"></script>
    <script src="../../Scripts/locales/es.js" type="text/javascript"></script>
    <script src="../../Content/themes/explorer-fa/theme.js" type="text/javascript"></script>
    <script src="../../Content/themes/fa/theme.js" type="text/javascript"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" type="text/javascript"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/js/bootstrap.min.js" type="text/javascript"></script>
</head>
<body>
    <form method="post">
        <input type="file"  />
        <input type="submit" />
    </form>
    <form action="" method="post" enctype="multipart/form-data">
        <fieldset>
            <legend> Upload Image </legend>
            <label for="Image">Image</label>
            <div class="form-group">
                <div class="file-loading">
                    <input id="files" type="file" multiple class="file" data-overwrite-initial="false" data-min-file-count="0">
                </div>
            </div>
            @*<div class="form-group">
                    <div class="file-loading">
                        <input id="Images" name="Images" type="file" multiple class="file" data-overwrite-initial="false">
                    </div>
                </div>*@
            <br />
            <input type="file" multiple />
            <input type="submit" value="Upload" id="file" name="file" />
        </fieldset>
    </form>
    <script>

        $("#files").fileinput({
            theme: 'fa',
            //uploadUrl: 'file:///D:/diplom/shop/shop.net/Shop/Images/Upload/', // you must set a valid URL here else you will get an error
            allowedFileExtensions: ['jpg', 'png', 'gif'],
            overwriteInitial: false,
            maxFileSize: 1000,
            maxFilesNum: 10,
            //allowedFileTypes: ['image', 'video', 'flash'],
            //slugCallback: function (filename) {
            //    return filename.replace('(', '_').replace(']', '_');
            //}
        });

    </script>
</body>
</html>

